obj  PersonA(age: i32, name: string) {
    field  age =  age
    field  name =  name
    hello(self) {
        print("ciallo ! my name is " +  self.name)
    }
    out_age(self) {
        print("I am " +  self.age + " years old")
    }
}
impl  PersonA -> public  {
    hello as  greeting,
    out_age as  age
}
