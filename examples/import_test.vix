import "std/arr.vix"
import "std/strings.vix"
extern "C"
{
    fn printf(fmt:ptr , ...) ->i32
}
fn main(argc:i32,argv:ptr) ->i32
{
    arr = [5 , 4 , 6 , 7 , 2 , 1]
    sort(arr , arr.length)
    for (i in 0 .. arr.length)
    {
        printf("%d\n",arr[i])
    }
    for (n in 1 .. argc)
    {
        if (strcmp(argv[n] , "--help") == 0)
        {
            printf("testing\n")
        }
    }
    return 0
}
