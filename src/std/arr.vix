pub fn sort(nums:[i32],size:i32)
{
    for(i in 0 .. size - 1)
    {
        for(j in 0 .. size - i - 1)
        {
            if (nums[j] > nums[j + 1])
            {
                temp = nums[j]
                nums[j] = nums[ j + 1 ];
                nums[j + 1] = temp
            }
        }
    }
}
fn main() ->i32
{
    arr = [5, 2, 8, 1, 9, 10]
    sort(arr , 5)
    for(i in 0 .. arr.length)
    {
        print(arr[i])
    }
    return 0
}
